{
    "env": {
        "node": true,
        "browser": true
    },
    "plugins": [
        "vue"
    ],
    "extends": [
        "eslint:recommended",
        "google",
        "plugin:vue/essential"
    ],
    "rules": {
        "require-jsdoc": "off"
    },
    "parserOptions": {
        "ecmaVersion": 2017
    }
}